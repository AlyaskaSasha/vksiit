"use strict";
let test = '12345'; 
alert(Number(test[0]) + Number(test[1]) + Number(test[2]) + Number(test[3]) + Number(test[4]));

test = String(12345); 
alert(Number(test[0]) + Number(test[1]) + Number(test[2]) + Number(test[3]) + Number(test[4]));

alert(test[0] * test[1] * test[2] * test[3] * test[4]);

let tset = test[4] + test[3] + test[2] + test[1] + test[0];
alert(tset);
